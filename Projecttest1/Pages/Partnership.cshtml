<form action="action_page.php" style="border:1px solid #ccc" id="signup">
    <div class="container">
        <h1>Sign Up</h1>
        <p>Please fill in this form to create an account.</p>
        <p>Join us now!</p>
        <hr>

        <label for="email"><b>Email</b></label>
        <input type="text" placeholder="Enter Email" name="email" required>

        <label for="psw"><b>Password</b></label>
        <input type="password" placeholder="Enter Password" name="psw" required>

        <label for="psw-repeat"><b>Repeat Password</b></label>
        <input type="password" placeholder="Repeat Password" name="psw-repeat" required>

        <label for="position"><b>Position</b></label><br>
        <input type="radio" name="position" value="volunteer" id="voluteer">
        <label for="volunteer">Volunteers</label>
        <input type="radio" name="position" value="staff" id="staff">
        <label for="staff">Staff</label>
        <input type="radio" name="position" value="intern" id="internship">
        <label for="intern">Intership</label>

        <br><br>
        <label>
            <input type="checkbox" checked="checked" name="remember" style="margin-bottom:15px"> Remember me
        </label><br>




        <p>By creating an account you agree to our <a href="#" style="color:dodgerblue">Terms & Privacy</a>.</p>

        <div class="clearfix">
            <button type="submit" class="signupbtn">Sign Up</button>
        </div>
    </div>
</form>
